{
  "name": "My test extension",
  "version": "1",
  "manifest_version": 2,
  "background": {
    "scripts":["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "tabs",
    "gcm",
    "storage",
    "notifications"
  ],
  "icons": { "128": "gcm_128.png" }

}